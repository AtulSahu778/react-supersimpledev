{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://urbancart-backend-etxw.onrender.com/api/:path*"
    },
    {
      "source": "/images/:path*",
      "destination": "https://urbancart-backend-etxw.onrender.com/images/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}